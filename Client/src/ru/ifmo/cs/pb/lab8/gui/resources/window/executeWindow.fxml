<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="300.0" prefWidth="380.0" xmlns="http://javafx.com/javafx/8.0.171"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.ifmo.cs.pb.lab8.gui.controller.EController">
   <cursor>
      <Cursor fx:constant="HAND"/>
   </cursor>
   <Label fx:id="label" alignment="CENTER" layoutX="186.0" layoutY="14.0" prefHeight="25.0"
          style="-fx-border-color: grey;" text="Execute" AnchorPane.leftAnchor="1.0" AnchorPane.rightAnchor="1.0"
          AnchorPane.topAnchor="1.0">
      <font>
         <Font size="15.0"/>
      </font>
   </Label>
   <AnchorPane layoutX="100.0" layoutY="62.0" prefHeight="200.0" prefWidth="200.0" style="-fx-border-color: grey;"
               AnchorPane.bottomAnchor="1.0" AnchorPane.leftAnchor="1.0" AnchorPane.rightAnchor="1.0"
               AnchorPane.topAnchor="28.0">
      <Button fx:id="runButton" layoutX="163.0" layoutY="233.0" mnemonicParsing="false" onAction="#runButtonAction"
              text="Run" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0"/>
      <TextArea fx:id="areaField" layoutX="9.0" layoutY="36.0" prefHeight="200.0" prefWidth="356.0"
                AnchorPane.bottomAnchor="45.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0"
                AnchorPane.topAnchor="70.0"/>
      <TextField fx:id="pathField" layoutX="13.0" layoutY="27.0" prefHeight="27.0" prefWidth="265.0"
                 AnchorPane.bottomAnchor="222.0" AnchorPane.leftAnchor="12.0" AnchorPane.topAnchor="26.0"/>
      <Button fx:id="fileButton" layoutX="312.0" layoutY="26.0" mnemonicParsing="false" onAction="#chooseFileAction"
              text="Choose File" AnchorPane.bottomAnchor="220.0" AnchorPane.rightAnchor="10.0"
              AnchorPane.topAnchor="25.0"/>
   </AnchorPane>
   <Hyperlink layoutX="341.0" layoutY="2.0" onAction="#closeButtonAction" text="x" textFill="#070707"
              AnchorPane.rightAnchor="5.0" AnchorPane.topAnchor="2.0"/>
</AnchorPane>
